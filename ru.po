msgstr "HERO: Юккакоппамаки!"

#: dialog0327.txt:101
msgid "$0: Giń!"
msgstr "$0: Умри!"

#: dialog0327.txt:102
msgid "$1: Zaraz umrzesz!"
msgstr "$1: Ты сейчас умрешь!"

#: dialog0327.txt:103
msgid "$2: Spróbuj tego!"
msgstr "$2: Попробуй!"

#: dialog0327.txt:104
msgid "$3: Yukkakoppamakki!"
msgstr "$3: Юккакоппамаки!"

#: dialog0327.txt:100001
